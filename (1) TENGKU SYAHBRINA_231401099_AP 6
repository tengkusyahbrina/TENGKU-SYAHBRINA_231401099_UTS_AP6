#include <iostream>

using namespace std;

int main() {
    int a, num = 1;

    cout << "Masukkan angka: ";
    cin >> a;

   
    for (int i = 1; i <= a; i++) {
        
        for (int j = 1; j <= i; j++) {
            cout << num << " ";
            num++;
        }
        cout << endl; 
    }

    return 0;
}
